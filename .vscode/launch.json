{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Storybook",
      "request": "launch",
      "type": "dart",
      "cwd": "storybook",
      "program": "lib/main.dart",
      "deviceId": "${input:deviceId}"
    }
  ],
  "inputs": [
    {
      "id": "deviceId",
      "type": "pickString",
      "description": "Select device",
      "options": [
        {
          "label": "macOS",
          "value": "macos"
        },
        {
          "label": "Windows",
          "value": "windows"
        },
        {
          "label": "Linux",
          "value": "linux"
        },
        {
          "label": "Android",
          "value": "android"
        },
        {
          "label": "iOS",
          "value": "ios"
        }
      ]
    }
  ]
}